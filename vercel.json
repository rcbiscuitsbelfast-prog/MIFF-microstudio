{
  "version": 2,
  "builds": [
    {
      "src": "api/vercel.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/api/vercel.js"
    },
    {
      "src": "/lib/(.*)",
      "dest": "/api/vercel.js"
    },
    {
      "src": "/css/(.*)",
      "dest": "/api/vercel.js"
    },
    {
      "src": "/js/(.*)",
      "dest": "/api/vercel.js"
    },
    {
      "src": "/img/(.*)",
      "dest": "/api/vercel.js"
    },
    {
      "src": "/fonts/(.*)",
      "dest": "/api/vercel.js"
    },
    {
      "src": "/lang/(.*)",
      "dest": "/api/vercel.js"
    },
    {
      "src": "/tutorials/(.*)",
      "dest": "/api/vercel.js"
    },
    {
      "src": "/doc/(.*)",
      "dest": "/api/vercel.js"
    },
    {
      "src": "/manifest.json",
      "dest": "/api/vercel.js"
    },
    {
      "src": "/robots.txt",
      "dest": "/api/vercel.js"
    },
    {
      "src": "/sitemap.xml",
      "dest": "/api/vercel.js"
    },
    {
      "src": "/sw.js",
      "dest": "/api/vercel.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/vercel.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
